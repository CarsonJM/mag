nextflow_workflow {

    name "Test workflow: MAG"
    script "../main.nf"
    workflow "MAG"
    tag "workflows"
    tag "mag"
    tag "mag_test_hybrid"
    config "../../../conf/mag_test_hybrid.config"

    test("Parameters: long reads included") {

